{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "outDir": "built",
    "rootDir": ".",
    "sourceMap": true,
    "skipLibCheck": true,
    "strictFunctionTypes": false
  },
  "include": [
    "src/*",
    "src/model/Physics.ts",
    "src/model/Rectangle.ts",
    "src/model/Game.ts",
    "src/model/Box.ts",
    "src/model/Ground.ts",
    "src/lib/box2d"
  ],
  "exclude": ["node_modules"]
}
